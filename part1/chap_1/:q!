from collections import namedtuple 

Card = namedtuple('Card', ['rank', 'suit'])

class FrenchDeck: 
	ranks = [str(n) for n in range(2, 11)] + list('JQKA')
	suits = 'spades diamonds clubs hearts'.split()

	def __init__(): 
		self._cards = [Card(*x) for x in zip(self.ranks, self.suits)]
		self._cards1 = [Card(rank, suit) for rank in self.ranks for suit in self.suits]
		print(self._cards)
		print(self._cards1) 


deck = FrenchDeck()
